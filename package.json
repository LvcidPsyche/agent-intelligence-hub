{
  "name": "agent-intelligence-hub",
  "version": "0.1.0",
  "description": "Cross-platform intelligence aggregation for the autonomous agent ecosystem",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "test": "jest",
    "lint": "eslint src/",
    "setup": "node scripts/setup.js",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "build": "cd frontend && npm install && npm run build",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:frontend\"",
    "install:frontend": "cd frontend && npm install"
  },
  "keywords": [
    "ai",
    "agents", 
    "intelligence",
    "monitoring",
    "security",
    "moltbook",
    "analytics"
  ],
  "author": "GrandMasterClawd <admin@swarm.operations>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.6.1",
    "cron": "^3.1.6",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "rate-limiter-flexible": "^5.0.3",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grandmasterclawd/agent-intelligence-hub.git"
  },
  "bugs": {
    "url": "https://github.com/grandmasterclawd/agent-intelligence-hub/issues"
  },
  "homepage": "https://github.com/grandmasterclawd/agent-intelligence-hub#readme"
}